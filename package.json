  "author": "ErrorTzy",
  "license": "MIT",
  "description": "Obsidian plugin for rendering Pandoc extended markdown syntax",
  "dependencies": {
    "@codemirror/language": "^6.11.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.1",
    "eslint-plugin-obsidianmd": "file:../eslint-plugin"
  },
  "devDependencies": {
    "@codemirror/lang-markdown": "^6.5.0",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "@wdio/cli": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.39.1",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "mocha": "^11.7.1",
    "obsidian": "^1.8.7",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.3",
    "wdio-obsidian-reporter": "^2.1.1",
    "wdio-obsidian-service": "^2.1.1"
  }
