{
  "name": "obsidian-pandoc-extended-markdown",
  "version": "1.4.9",
  "main": "index.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "test": "jest",
    "test:e2e": "wdio run ./wdio.conf.mts",
    "test:e2e:dev": "wdio run ./wdio.conf.mts --logLevel=info",
    "test:all": "npm test && npm run test:e2e"
  },
  "keywords": [
    "obsidian",
    "pandoc",
    "markdown",
    "extended",
    "lists",
    "superscript",
    "subscript"
